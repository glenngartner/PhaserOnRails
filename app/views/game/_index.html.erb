    game.load.image('toilet', '../assets/Toilet.png');
    game.load.image('star01', '../assets/star.svg')
  }

  function create() {

    // create board assets
    game.stage.backgroundColor = '#FFFFFF';
    var background = game.add.sprite(0, 0, 'background_space');

    var spaceship = newDraggableToken(spaceship, 15, 75, 'spaceship', .65, .65);

    var toilet = newDraggableToken(toilet, 5, 175, 'toilet', 0.5, 0.5);

    var star01 = newDraggableToken(star01, 5, 250, 'star01', 1.5, 1.5);

    var smile01 = newDraggableToken(smile01, 15, 15, 'reward_smile', 2, 2);

    var smile02 = newDraggableToken(smile02, 15, 325, 'reward_smile', 2, 2);


  }

  function update() {
  }

</script>
